    <validationRules>
        <fullName>Account_Plan_Verification_Check</fullName>
        <active>false</active>
        <description>Check if Account for Plan Account is verified</description>
        <errorConditionFormula>AND( 
$User.ProfileId &lt;&gt; $Label.Profile_Sys_Admin_3rd_Party , 
$User.ProfileId &lt;&gt; $Label.Profile_Sys_Admin , 
Plan_Account__r.RecordTypeId &lt;&gt; $Label.RT_AccountVerified 
)</errorConditionFormula>
        <errorMessage>The Account you have selected is not verified. Please submit an Einstein ticket to Data Quality to verify your account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Editing_Restricted</fullName>
        <active>true</active>
        <description>as per case : 06842281 : Approval status should be edited by Mgr and above. Last updated RC-34489 SFDC-4331
As per case 11012161, RC-38950 to check if the Role contains values MGR.</description>
        <errorConditionFormula>AND( 
$User.ProfileId &lt;&gt; $Label.Profile_Sys_Admin, 
$User.ProfileId &lt;&gt; $Label.Profile_Sys_Admin_3rd_Party, 
$User.ProfileId &lt;&gt; $Label.Profile_Sys_Admin_No_Cust , 
NOT( 
OR( 
/*TO DO:Custom Label to store accepted values*/ 
ISPICKVAL($User.Level__c, &apos;SrMgr&apos;), 
ISPICKVAL($User.Level__c, &apos;Mgr&apos;), 
ISPICKVAL($User.Level__c, &apos;Dir&apos;), 
CONTAINS(UPPER($UserRole.Name), &apos;MGR&apos;), 
CONTAINS($UserRole.Name, &apos;Dir&apos;))), 
NOT($Setup.OverrideRulesSetup__c.OverrideValidationRule__c), 

ISCHANGED( Approval_Status__c) 
)</errorConditionFormula>
        <errorDisplayField>Approval_Status__c</errorDisplayField>
        <errorMessage>Manager and above can update the Approval Status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enter_Justification_for_Candidate_Type</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 OR( 
 ISPICKVAL(Red_Candidate__c , &apos;Red&apos;),
 ISPICKVAL(Red_Candidate__c , &apos;Green&apos;)),
 OR(ISBLANK(Red_Account_Justification__c ),
 ISPICKVAL(Amount_USD__c , &apos;&apos;)),
 NOT($Setup.OverrideRulesSetup__c.OverrideValidationRule__c) )</errorConditionFormula>
        <errorMessage>Please enter Justification and Amount USD for Candidate Type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enter_Justification_for_Red_Account</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
OR(ISPICKVAL(Red_Account_Status__c, &quot;Accept&quot;),
ISPICKVAL(Red_Account_Status__c,&quot;Nominate&quot;), 
ISPICKVAL(Red_Account_Status__c,&quot;Reject&quot;)), 
ISBLANK( Red_Account_Justification__c ))</errorConditionFormula>
        <errorDisplayField>Red_Account_Justification__c</errorDisplayField>
        <errorMessage>Please enter in a Justification for the Red Account Status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_Risk_Reason_Req_If_Reason_Is_Other</fullName>
        <active>true</active>
        <description>If Risk Reason is Other, Other Risk Reason is required</description>
        <errorConditionFormula>AND( 
ISPICKVAL( Risk_Reason__c , &apos;Other&apos;), 
Other_Reason__c  = &apos;&apos; , 
$User.ProfileId &lt;&gt; $Label.Profile_Sys_Admin, 
$User.ProfileId &lt;&gt; $Label.Profile_Sys_Admin_3rd_Party, 
$User.ProfileId &lt;&gt; $Label.Profile_Sys_Admin_No_Cust 
)</errorConditionFormula>
        <errorMessage>If Risk Reason is Other, you must manually input the reason in Other Risk Reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Plan_Year_Check</fullName>
        <active>true</active>
        <description>For LMS Account Plans, Plan Year cannot be blank.</description>
        <errorConditionFormula>AND( 
$Profile.Id &lt;&gt; $Label.Profile_Sys_Admin, 
$Profile.Id &lt;&gt; $Label.Profile_Sys_Admin_3rd_Party, 
$Profile.Id &lt;&gt; $Label.Profile_Sys_Admin_No_Cust, 
RecordType.DeveloperName = &apos;LMS Account Plans&apos;, 
ISBLANK(TEXT( Plan_Year__c)) 
)</errorConditionFormula>
        <errorDisplayField>Plan_Year__c</errorDisplayField>
        <errorMessage>Please include a year for the Account Plan</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Primary_contact_field_required</fullName>
        <active>true</active>
        <description>Make primary contact field required once lifecycle stage is Plan complete or Success Review or if already created new record at that stage.</description>
        <errorConditionFormula>AND( 

$Profile.Id &lt;&gt; $Label.Profile_Sys_Admin, 
$Profile.Id &lt;&gt; $Label.Profile_Sys_Admin_3rd_Party, 
$Profile.Id &lt;&gt; $Label.Profile_Sys_Admin_No_Cust, 

OR( 
ISNEW(), 
ISCHANGED( Status__c ) 
), 
ISBLANK( Primary_contact__c ), 
OR( 
ISPICKVAL(Status__c ,&apos;Plan complete&apos;), 
ISPICKVAL(Status__c ,&apos;Success Review&apos;) 
) 
)</errorConditionFormula>
        <errorDisplayField>Primary_contact__c</errorDisplayField>
        <errorMessage>Primary contact field is required when lifecycle stage is Plan complete or Success Review.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Risk_Outcome_Required_Status_Closed</fullName>
        <active>true</active>
        <description>Risk Outcome is required when status is closed</description>
        <errorConditionFormula>AND(
RecordTypeId = $Label.LSS_CS_Churn_Mitigation,
ISPICKVAL(Status__c, &apos;Closed&apos;),
ISPICKVAL( Mitigation_Outcome__c , &apos;&apos;),
$User.ProfileId &lt;&gt; $Label.Profile_Sys_Admin, 
$User.ProfileId &lt;&gt; $Label.Profile_Sys_Admin_3rd_Party, 
$User.ProfileId &lt;&gt; $Label.Profile_Sys_Admin_No_Cust
)</errorConditionFormula>
        <errorDisplayField>Mitigation_Outcome__c</errorDisplayField>
        <errorMessage>Mitigation Outcome is required when Status is Closed</errorMessage>
    </validationRules>
